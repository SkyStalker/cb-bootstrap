component accessors="true" extends="BaseController" {	public void function onApplicationStart(event){		}	public void function onApplicationEnd(event){	}	public void function onRequestStart(event){	}	public void function onRequestEnd(event){	}	public void function onSessionStart(event){			}	public void function onSessionEnd(event,rc,prc){		var sessionScope = event.getValue("sessionReference");		var applicationScope = event.getValue("applicationReference");	}	public void function onException(event,rc,prc){		var exception = event.getValue("ExceptionBean"); 		 		logger.error(exception.getMessage(),exception); 		if( variables.environment == "PRODUCTION" ){ 			setNextEvent(event='error');	 		}	}	public void function onInvalidEvent(event,rc,prc){		logger.info("PageNotFound: #rc.event#");		// this event does not exist, 404		event.setHTTPHeader('404','Page Not Found');		event.setView('pageNotFound');	}}